        <div class="row align-items-center">
            <div class="col-lg-3 col-md-4 col-sm-4 col-5">
                <div class="fit_logo" >
                    <a href="#">
                        <img src="/assets/totalgym.png" class="img" alt="logo" title="logo" style="max-width:100px;">
                    </a>
                </div>
            </div>
            <div class="col-lg-9 col-md-8 col-sm-8 col-7">
                <div class="fit_main_menu main_menu_parent">
                    <!-- Header Menus -->
                    <div class="fit_nav_items main_menu_wrapper text-right">
                        <ul>
                            <li class="mobile_logo"><a style="text-decoration:none;" href="javascript:void(0);"><img src="assets/images/logo.png" class="img-fluid" alt="logo" title="logo"></a></li>
                            <li ><a style="text-decoration:none;" href="#">Inicio</a></li>
                            <li><a style="text-decoration:none;" href="#">Acerca de Nosotros</a></li>
                            <li><a style="text-decoration:none;" href="#">Membresías</a></li>
                            <li class="has_submenu" >
                                <a style="text-decoration:none;" href="#">Horario</a>
                            </li>
                            <li><a style="text-decoration:none;" href="#">Contactanos</a></li>
                        </ul>
                    </div>
                    <div class="fit_search_wrapper menu_btn_wrap">
                        <span class="fit_searchbtn_wrap">
                            <a href="javascript:void(0);" class="fit_search_btn searchbtn">

