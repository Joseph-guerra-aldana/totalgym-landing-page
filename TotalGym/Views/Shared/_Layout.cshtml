                            <li>
                                <a href="javascript:void(0);" class="menu_btn toggle_btn">
                                    <span></span>
                                    <span></span>
                                    <span></span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>

<!-- Header End -->
<body>

    <!-- preloader start -->
    <div id="preloader" style="display: none;">
        <div id="status" style="display: none;">
            <img src="assets/images/preloader.gif" alt="preloader">
        </div>
    </div>
    <!-- preloader end -->
    <!-- Banner section start -->
    <section class="fit_banner_wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-6 col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="fit_banner_text">
                        <h1>Desafia tus limites</h1>
                        <h3 class="pb-3">Inscribete hoy y recibe <span class="h2 text-warning">3 MESES GRATIS</span></span></h3>
                        <div class="banner_btn_wrap mt-2">
                            <a href="javascript:void(0);" class="fit_btn text-light" style="text-decoration:none;">Inscribirme</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Banner section end -->
    <div class="container-fluid mx-0 px-0">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <a href="https://api.whatsapp.com/send?phone=50250937829&text=Quiero%20programar%20mi%20clase%20de%20prueba" class="btn-wsp" target="_blank">
        <i class="bi bi-whatsapp text-light" data-toggle="tooltip" title="Escribenos"></i>
    </a>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - TotalGym - <a asp-area="" asp-controller="Home" asp-action="Privacy">Este Sitio fue desarrollado por Joseph Aldana</a>
        </div>
    </footer>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery-3.5.1.slim.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.magnific-popup.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.min.js" asp-append-version="true"></script>
    <script src="~/js/nice-select.min.js" asp-append-version="true"></script>
    <script src="~/js/SmoothScroll.min.js" asp-append-version="true"></script>
    <script src="https://kit.fontawesome.com/939476ebfb.js" crossorigin="anonymous" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
